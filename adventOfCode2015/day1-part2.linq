int index = 0, upcount = 0, downcount = 0;
foreach (var ch in input)
{
	index++;
	if (ch == '(')
	{
		upcount++;
	}
	else if (ch == ')')
	{
		downcount++;
	}
	else
	{
		throw new Exception("Oops. Bad character at index " + index.ToString());
	}

	if (upcount == (downcount - 1))
	{
		Console.WriteLine("Basement level 1 at position {0}; Upcount = {1}; Downcount = {2}", index, upcount, downcount);
	}
